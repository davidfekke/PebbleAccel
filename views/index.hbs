<h1>{{title}}</h1>
<p>Welcome to {{title}}</p>

<div>
  <span id="x-axis"></span>
  <span id="y-axis"></span>
  <span id="z-axis"></span>
</div>

<script src="http://code.jquery.com/jquery-1.11.3.min.js"></script>
<script src="/socket.io/socket.io.js"></script>
<script>
  var socket = io();
  socket.on('acceldata', function (data) {
    $('#x-axis').text('x: ' + data.x);
    $('#y-axis').text('y: ' + data.y);
    $('#z-axis').text('z: ' + data.z);
  });
</script>